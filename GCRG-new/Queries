CREATE TABLE atq_no_gap_filled_cleaned (time_stamp TIMESTAMP(1),
H	Double,
qc_H	Double,
LE	Double,
qc_LE	Double,
co2_flux	Double,
h2o_flux	Double,
ch4_flux	Double,
qc_ch4_flux	Double,
co2_mixing_ratio	Double,
h2o_mixing_ratio	Double,
ch4_mixing_ratio	Double,
air_pressure	Double,
RH	Double,
VPD	Double,
wind_speed	Double,
wind_dir	Double,
uStar	Double);


CREATE TABLE atq_Meteorological (time_stamp TIMESTAMP(1),
PAR_AVG_L	Double,
Rsolar_AVG_L	Double,
Rnet_WC_AVG_L	Double,
Air_T_AVG_L	Double,
RH_AVG_L	Double,
G_1_AVG_L	Double,
G_2_AVG_L	Double,
G_3_AVG_L	Double,
PPT_TOT_L	Double,
Press_mb_AVG_L	Double,
SS_Tl_R_AVG_L	Double,
SS_Dif_R_AVG_L	Double,
SWC_1_AVG_L	Double,
SWC_2_AVG_L	Double,
SWC_3_AVG_L	Double,
SWC_4_AVG_L	Double,
P2_SWC_5_AVG_L	Double,
P2_SWC_15_AVG_L	Double,
P2_SWC_30_AVG_L	Double,
P3_SWC_5_AVG_L	Double,
P3_SWC_15_AVG_L	Double,
P3_SWC_30_AVG_L	Double,
P3_SolT5_AVG_L	Double,
P3_SolT15_AVG_L	Double,
P3_SolT30_AVG_L	Double,
P4_SolT5_AVG_L	Double,
P4_SolT15_AVG_L	Double,
P4_SolT30_AVG_L	Double,
SnowDepth_L	Double);


CREATE TABLE atq_Gap_Filled (time_stamp TIMESTAMP(1),
 NEE_f	Double,
 NEE_fqcOK	Double,
 LE_f	Double,
 LE_fqcOK	Double,
 H_f	Double,
 H_fqcOK	Double,
 Reco	Double,
 GPP_f	Double);

Create Table users(user_id int NOT NULL AUTO_INCREMENT, time_stamp TIMESTAMP, first_name varchar(25), last_name varchar(25), password varchar(25), email varchar(25), phone varchar(25), PRIMARY KEY(user_id));

Create table contacts (user_id int, name varchar(25) not null, role varchar(25), phone varchar(25), email varchar(25), primary key(user_id), FOREIGN KEY (user_id) 
        REFERENCES users(user_id)) ;